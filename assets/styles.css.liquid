/* BASE */
html,
body {
    background: #f5f4f0 url({{ 'background.png' | asset_url }}) bottom repeat-x;
    color: #5d5a4b;
    font-family: 'Roboto', sans-serif;
}

nav,
.navbar-default,
footer.footer {
    background-color: transparent;
}

article {
    margin-top: 30px;
    margin-bottom: 30px;
}

hr {
    border-bottom: 2px solid #C0BFBB;
}

small {
    font-size:.7em;
}

.container {
    position: relative;
}

@media (max-width: 960px) {
    .container {
        width: auto;
    }
}

.container-absolute {
    position: absolute;
    width: 100%;
    left: 50%;
    transform: translate(-50%,0);
    -webkit-transform: translate(-50%,0);
    -ms-transform: translate(-50%,0);
    -moz-transform: translate(-50%,0);
    top: 0px;
    bottom: 0px;
}

@media (min-width: 961px) {
    .container-absolute,
    .container {
        width: 960px;
    }
}

.preloader {
    width: 100%;
    height: 100%;
    background: url({{ 'spinner.gif' | asset_url }}) no-repeat center center;
}

.placeholder {
    width: 100%;
    height: 100%;
    position: relative;
    background-color: #353535;
}

.placeholder.large:after {
    font-size: 5em;
}

.placeholder:after {
    content: "\f03e";
    font-family: 'FontAwesome';
    font-size: 12px;
    position: absolute;
    background-color: #353535;
    color: rgb(87, 87, 87);
    text-align: center;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
}

.well-lg {
    border-radius: 0;
    padding: 48px 24px;
}
.well {
    background-color: #353535;
    border: 0px;
    border-radius: 0px;
    -webkit-box-shadow: none;
    box-shadow: none;
    color: white;
}

.page-header {
    color: #f5f4f0;
    background-color: #353535;
    padding: 15px;
    text-align: center;
    margin-top: 0px;
}

.page-header h1 {
    font-weight: 100;
    font-size: 1.8em;
    margin: 0px;
    letter-spacing: 1px;
}

.breadcrumb {
    text-transform: uppercase;
    letter-spacing: 1px;
    border-bottom: solid 2px #c0bfbb;
    padding-bottom: 20px;
    padding-top: 0px;
    letter-spacing: 3px;
}

.breadcrumb > li + li:before {
    content: "> ";
}

.breadcrumb > li.active {
    font-weight: bold;
}

.pagination > .active > a,
.pagination > .active > span,
.pagination > .active > a:hover,
.pagination > .active > span:hover,
.pagination > .active > a:focus,
.pagination > .active > span:focus,
.pagination > li > a:hover,
.pagination > li > a:focus,
.pagination > li > span:hover,
.pagination > li > span:focus {
    color: #F5F4F0;
    background-color: #575757;
    border-color: #F5F4F0;
}

.pagination > li > a,
.pagination > li > span {
    color: #F5F4F0;
    background-color: #353535;
    border-color: #F5F4F0;
    font-weight: 100;
}

.pagination li a {
    max-height: 34px;
}

.pagination > .disabled > span,
.pagination > .disabled > span:hover,
.pagination > .disabled > span:focus,
.pagination > .disabled > a,
.pagination > .disabled > a:hover,
.pagination > .disabled > a:focus {
    display: none;
}

.st {
    width: 61px;
    height: 25px;
    background-size: 61px 25px;
    background-repeat: no-repeat;
    cursor: pointer;
    display: block;
}

.st_facebook_custom {
    background-image: url({{ 'share.png' | asset_url }})
}

.st_twitter_custom {
    background-image: url({{ 'tweet.png' | asset_url }})
}

.st_pinterest_custom {
    background-image: url({{ 'pin.png' | asset_url }})
}

.thumbnail {
    background-color: transparent;
    border: none;
    border-radius: inherit;
}

.pager li > a, .pager li > span {
    border-radius: 0px;
}

/* OVERRIDES */
@media (min-width: 768px) {
    .row.half-gutter {
        margin-right: -7.5px;
        margin-left: -7.5px;
    }

    .row.half-gutter .col {
        padding-left: 7.5px;
        padding-right: 7.5px;
    }

    .cheat-gutter {
        margin-left: -15px;
        margin-right: 15px;
    }
}

.right-dotted {
    border-right: dotted 1px #353535;
}

.bottom-dotted {
    border-bottom: dotted 1px #353535;
}

.left-dotted {
    border-left: dotted 1px #353535;
}

/* BUTTONS */
.btn {
    text-transform: uppercase;
    letter-spacing: 1px;
    padding: 15px 35px;
    transition: all 0.3s ease;
    padding: 13px 35px;
}

a {
    transition: all 0.1s ease-in-out 0s;
}

.btn-default,
.btn-default:hover,
a.btn-default,
a:link.btn-default,
a:hover.btn-default,
a:active.btn-default,
a:visited.btn-default {
    color: white;
    background-color: #353535;
}

.btn-default:hover,
.btn-default:active,
a:hover.btn-default,
a:active.btn-default {
    background-color: #484848;
}

.btn-default:hover,
.btn-default:focus,
.btn-default.focus,
.btn-default:active,
.btn-default.active,
.open > .dropdown-toggle.btn-default {
    background-color: #484848;
    color: white;
}

.center-underlined {
    text-align: center;
    border-bottom: solid 1px #dbdbda;
    padding-bottom: 15px;
    margin-bottom: 30px;
}

/* FONTS */

html .fa {
    font-size: 1.3em;
    margin: 0 4px;
}

a,
a:link,
a:hover,
a:active,
a:visited {
    color: #353535;
    text-decoration: none;
}

a:hover {
    text-decoration: underline;
}

h1, h2, h3, h4 {
    font-weight: normal;
}

h1, h2, h3, h5 {
    text-transform: uppercase;
    letter-spacing: 1px;
}

h4 .fa {
    margin-right: 10px;
}

/* FORMS */
.form-group {
    margin-bottom: 7px;
}

.form-control {
    padding: 6px 12px;
}

input.search {
    box-sizing: border-box;
    background: url('icon-search.png') no-repeat 8px center;
    background-size: 14px;
    padding: 6px 12px 6px 30px;
}

label {
    font-weight: normal;
}

.form-control::-webkit-input-placeholder,
.form-control:-moz-placeholder,
.form-control::-moz-placeholder,
.form-control:-ms-input-placeholder {
    color: #cfcecb;
}

.checkbox {
    padding-left: 14px;
}

/* ALERTS */

.flash.alert {
    margin-bottom: 0px;
    border-radius: 0px;
    text-align: center;
    text-transform: uppercase;
    letter-spacing: 1px;
    font-size: 1.1em;
    padding: 5px;
}

.flash.alert-info {
    color: white;
    background-color: #353535;
    border: 0px;
}

/* NAVBAR */
.navbar-brand {
    position: absolute;
    width: 100%;
    left: 0;
    text-align: center;
    margin: 0 auto;
}

@media (min-width: 768px) {
    .navbar-brand {
        margin: 30px auto 0 auto;
    }


    .navbar > .container .navbar-brand,
    .navbar > .container-fluid .navbar-brand {
        margin-left: 0px;
    }

    .navbar-brand > a {
        margin: 0 auto;
        display: block;
        width: 301px;
    }

    .navbar-brand > img {
        margin: 0 auto;
    }

    .nav.main-nav > li {
        padding-left: 0px;
        padding-right: 0px;
        margin-right: 13px;
        margin-left: 13px;
    }

    .main-nav {
        margin-top: 65px;
        width: 100%;
        background-color: #f5f4f0;
    }

    body.nav-affixed {
        padding-top: 112px;
    }

    body.nav-affixed .navbar-right,
    body.nav-affixed .navbar-brand,
    body.nav-affixed .navbar-left {
        display: none;
    }

    .navbar-default .main-nav.affix {
        margin-top: 0px;
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        padding-bottom: 8px;
    }

    .main-nav li {
        float: none;
        display: inline-block;
    }

    .navbar {
        margin-bottom: 0px;
    }

    .navbar-right {
        position: absolute;
        top: 0;
        right: 22px;
    }

    .navbar-default .main-nav.navbar-nav {
        margin-bottom: 3px;
    }

    .navbar-default .navbar-right .navbar-nav {
        padding: 0px;
    }

    .navbar-right li {
        padding: 0 2px;
    }

    .navbar-right li.cart-link {
        margin-bottom: 8px;
        padding-left: 0px;
        padding-top: 0px;
    }

    .navbar-right .list-inline {
        margin-left: 1px;
    }

    .navbar-right li.cart-link a {
        padding: 15px 0px 5px 0px
    }

    .navbar-right .fa-shopping-cart {
        margin-right: 11px;
        position: relative;
        top: 5px;
    }

    .navbar-right .fa {
        font-size: 1.6em;
    }

    .navbar-nav {
        width: 100%;
        text-align: center;
        text-transform: uppercase;
        letter-spacing: 1px;
    }

    .navbar-default .navbar-nav {
        margin-bottom: 7px;
        padding: 5px;
    }

    .navbar-default .main-nav.navbar-nav > li > a {
        background-color: transparent;
    }

    .navbar-default .main-nav.navbar-nav > li > a:hover {
        background-color: transparent;
    }

    .navbar-default .main-nav.navbar-nav > li > a:before {
        content: "";
        position: absolute;
        width: 100%;
        height: 2px;
        bottom: 0;
        left: 0;
        background-color: #353535;
        visibility: hidden;
    }

    .navbar-default .main-nav.navbar-nav > li.active > a:before,
    .navbar-default .main-nav.navbar-nav > li.open > a:before,
    .navbar-default .main-nav.navbar-nav > li > a:hover:before,
    .navbar-default .main-nav.navbar-nav > .active > a:focus:before {
        visibility: visible;
        -webkit-transform: scaleX(1);
        transform: scaleX(1);
    }

    .navbar-default .main-nav.navbar-nav > .open > a,
    .navbar-default .main-nav.navbar-nav > .open > a:hover,
    .navbar-default .main-nav.navbar-nav > .open > a:focus {
        background-color: transparent;
    }

    .nav > li {
        padding-left: 10px;
        padding-right: 10px;
    }

    .nav > li > a {
        padding: 10px 5px;
    }

    nav input.search {
        margin-top: 15px;
        font-style: italic;
        z-index: 200;
    }

    .dropdown-menu {
        position: absolute;
        top: 48px;
        left: 50%;
        right: auto;
        transform: translate(-50%, 0);
        -webkit-transform: translate(-50%, 0);
        -ms-transform: translate(-50%, 0);
        -moz-transform: translate(-50%, 0);
        z-index: 1000;
        display: none;
        float: left;
        min-width: 160px;
        margin: 2px 0 0;
        font-size: 14px;
        text-align: left;
        list-style: none;
        background-color: rgba(53, 53, 53, .9);
        -webkit-background-clip: padding-box;
        background-clip: padding-box;
        border: 1px solid #ccc;
        border: 1px solid rgba(0, 0, 0, .15);
        -webkit-box-shadow: 0 6px 12px rgba(0, 0, 0, .175);
        box-shadow: 0 6px 12px rgba(0, 0, 0, .175);
        width: 100%;
        padding-top: 5px;
        padding-bottom: 5px;
    }

    .dropdown-menu:after {
        left: 83px;
    }

    .dropdown-menu li {
        width: 100%;
        display: block;
        text-align: center;
        padding: 8px 0;
    }

    .dropdown-menu a {
        padding: 0 40px;
        display: block;
        width: 100%;
    }

    .dropdown-menu a,
    .dropdown-menu a:visited,
    .dropdown-menu a:hover,
    .dropdown-menu a:active {
        background-color: transparent !important;
        color: white !important;
    }

    .dropdown-menu a,
    .dropdown-menu a:hover,
    .dropdown-menu a:visited,
    .dropdown-menu a:active {
        color: white;
        font-weight: bold;
    }
}

/* SLIDESHOW */
.home .slider {
    display: none;
    overflow: hidden;
}

.home .slick-slider > div {
    max-height: 450px !important;
}

.home .slider .slide {
    position: relative;
    background-size: cover !important;
}

.home .slider .slick-track,
.home .slider .slick-slide {
    max-height: 450px !important;
}

.home .slide:before {
    content: "";
    display: block;
    padding: 25%;
    box-sizing: border-box;
    max-height: 1119px;
}

@media (min-width: 1200px) {
    .home .slider .slide {
        position: relative;
        background-position: top;
    }
}

.home .slider .slide .caption .st_pinterest_custom {
    position: absolute;
    bottom: -40px;
    right: 0px;
    display: none;
}

.home .slider .slide:hover .caption .st_pinterest_custom {
    display: block;
}

.home #slider-preloader {
    margin-bottom: 30px;
}

.home #slider-preloader img {
    height: 551px;
}

.home .slider .caption {
    width: 300px;
    background-color: rgba(245, 244, 240, .9);
    padding: 30px;
    float: right;
    margin: 40px 75px;
    display: none;
    position: relative;
}

.home .slider .caption h4 {
    font-size: 2.4em;
    font-weight: 100;
}

.home .slider .caption .description {
    margin: 22px 0 32px
}

.home .slider ul.slick-dots {
    bottom: 25px;
}

.home .slider ul.slick-dots li button {

    text-indent: -9999px;
    border-radius: 5px;
    border: solid 2px #cecbcc;
    transition: all 0.3s ease-out;
}

.home .slider ul.slick-dots li:hover button,
.home .slider ul.slick-dots li.slick-active button {
    background-color: #b2b1b2;
}

.home .slider .slick-next,
.home .slider .slick-prev {
    height: 30px;
    bottom: 30px;
    top: auto;
    z-index: 200;


}

.home .slider .slick-prev {
    left: 30px;
}

.home .slider .slick-next {
    right: 30px;
}

.slick-prev:before  {
    font-family: 'FontAwesome';
    content: "\f053";
    font-size: 30px;
}

.slick-next:before {
     font-family: 'FontAwesome';
     content: "\f054";
    font-size: 30px;
 }

.home .slider .pin-it {
    position: absolute;
    bottom: -40px;
    right: 0px;
    display: none;
}

.home .slider .slide:hover .pin-it {
    display: inline-block;
}

/* HOME COLLECTIONS */
.home .collections {
    margin-bottom: 20px;
}

.home .collection {
    position: relative;
    cursor: pointer;
    height: 195px;
}

.home .collection > div {
    overflow: hidden;
}

.home .collection h5 {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%,-50%);
    -ms-transform: translate(-50%,-50%);
    -moz-transform: translate(-50%,-50%);
    color: white;
    text-transform: uppercase;
    letter-spacing: 1px;
    font-size: 1.5em;
    text-align: center;
    font-weight: normal;
    display: block;
    width: 205px;
    padding: 15px;
    border: solid 2px white;
    margin: 0px;
    -webkit-transition: all 0.3s ease-out;
    transition: all 0.3s ease-out;
    cursor: pointer;
    box-sizing: border-box;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
}

.home .collection img {
    -webkit-transform: scale(1);
    transform: scale(1);
    opacity:1;
    -webkit-transition:-webkit-transform .3s ease-in, opacity .3s ease-out;
    transition:transform .3s linear, opacity .3s ease-out;
    -webkit-backface-visibility: hidden;
}

@media (min-width: 768px) {
    .home .collection:hover h5 {
        color: #353535;
        border: solid 2px #353535;
        text-shadow: none;
    }

    .home .collection:hover img {
        -webkit-transform: scale(1.6);
        transform: scale(1.6);
        -webkit-transition:-webkit-transform 4s ease-in, opacity .3s ease-out;
        transition:transform 4s linear, opacity .3s ease-out;
        opacity: 0.3;
    }
}

/* HOME GALLERY THUMBNAILS */
.home-gallery-thumbnail,
.home-gallery-thumbnail > div {
    cursor: pointer;
    height: 174px;
    overflow: hidden;
}

.home-gallery-thumbnail > div {
    position: relative;
}

.home-gallery-thumbnail h5 {
    position: absolute;
    left: 0;
    right: 0;
    top: 50%;
    height: 87px;
    transform: translate(0, -50%);
    -webkit-transform: translate(0, -50%);
    -ms-transform: translate(0, -50%);
    -moz-transform: translate(0,-50%);
    color: white;
    text-transform: uppercase;
    letter-spacing: 1px;
    font-size: 1em;
    text-align: center;
    font-weight: normal;
    display: block;
    padding: 19px;
    margin: 0px;
    -webkit-transition: all 0.3s ease-out;
    transition: all 0.3s ease-out;
    cursor: pointer;
    box-sizing: border-box;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
    z-index: 200;
    letter-spacing: 4px;
    background-color: rgba(53, 53, 53, .5);
}

@media (min-width: 768px) {
    .home-gallery-thumbnail:hover {
        transform: scale(1.1);
    }

    .home-gallery-thumbnail:hover h5 {
        text-shadow: none;
        color: #353535;
        background-color: rgba(245, 244, 240, .5);
    }
}


.home-gallery-thumbnail i {
    display: block;
    margin-bottom: 10px;
    font-size: 1.3em;
}

.home-gallery-thumbnail i:before {
    font-size: 1.4em;
}

/* CUSTOMERS */
.customers .customer {
    margin-bottom: 50px;
    text-align: center;
}

.customers .customer h5 {
    margin-top: 1px;
}

/* NEWS */
.news h4 {
    margin-top: 20px;
    margin-bottom: 20px;
    font-size: 1.4em;
    font-weight: normal;
}

.news .btn {
    margin-top: 40px;
}

.news .fa-newspaper-o {
    font-size: 1.1em;
}

.news .fa-newspaper-o:before {
    position: relative;
    top: 3px;
}

/* CONNECT */
#mc_embed_signup {
    margin-bottom: 15px;
}

#mc_embed_signup input.mce_inline_error {
    background-color: rgb(255, 253, 234);
    border-color: red !important;
    border: solid 1px red;
}

#mc_embed_signup .danger,
#mc_embed_signup .success {
    font-style: italic;
    margin-bottom: 10px;
}

#mc_embed_signup .error {
    color: red;
}

#mc_embed_signup .newsletter-label {
    margin-left: 7px;
}

.contact .location {
    margin-bottom: 30px;
}

.contact #mc-embedded-subscribe.btn-block {
    width: auto;
    margin: 0 auto;
    padding: 10px 70px;
}

/* MAP */
.map-container {
    margin-bottom: 15px;
    width: 100%;
    height: 200px;
}

.map-container iframe {
    border:0;
    width: 100%;
    height: 100%;
}

.home .map-container {
    height: 125px;
}

.directions-link {
    float: right;
}

.home .location {
    text-align: left;
}

/* STORE HOURS */
.store-hours .center-underlined {
    margin-bottom: 20px;
}

/* FOOTER */
footer.footer {
    margin-top: 0px;
    border-top: 0px;
}

.footer-nav {
    padding: 9px 0;
    background-color: #353535;
}

.footer-nav a,
.footer-nav a:link,
.footer-nav a:hover,
.footer-nav a:active,
.footer-nav a:visited {
    color: white;
}

.footer-nav li {
    padding: 0px 21px;
    text-transform: uppercase;
    letter-spacing: 1px;
    font-size: 1.2em;
}

footer .payment-methods {
    font-size: 2.1em;
    margin: 25px 0 15px;
}

footer .payment-methods .fa {
    margin: 0px;
}

/* GALLERY & BLOG */
.blog-view .wrapper {
    position:relative;
    margin:0 auto;
    height: 200px;
}

.blog-post #post,
.gallery .list,
.gallery #post {
    display: none;
}

.blog-post #post .body,
.gallery #post .body {
    padding: 0 15px;
}

.gallery .list .slide {
    position: relative;
    height: 185px;
}

.gallery .list .slide a {
    height: 170px;
    display: block;
    margin-bottom: 15px;
    padding: 3%;
    box-sizing: border-box;
  	text-decoration: none;
}

.gallery .list .slide .image {
    background-size: cover;
    background-position: center center;
    height: 148px;
    background-repeat: no-repeat;
    background-color: #353535;
    position: relative;
}

.gallery .list .slide .image:before {
    content: ' ';
    height: 100%;
    width: 100%;
    background-color: #353535;
    position: absolute;
    opacity: .5;
    transition: all 0.3s ease-out;
}

.gallery .list a:hover .image:before,
.gallery .list a.active .image:before {
    display: none;
}

.gallery .list a.video:hover .image:after,
.gallery .list a.active.video .image:after {
    content: "\f144";
    font-family: 'FontAwesome';
    font-size: 45px;
    position: absolute;
    color: #f5f4f0;
    text-align: center;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
}

.gallery .list a:hover .placeholder:after,
.gallery .list a.active .placeholder:after {
    color: rgb(126, 126, 126);
}

.blog-post #post-preloader,
.gallery #post-preloader {
    min-height: 200px;
}

.gallery #post-preloader {
    display: none;
}

.gallery .slide .caption {
    text-align: center;
    padding: 8px;
}

.gallery .slick-prev,
.gallery .slick-next {
    top: 45%;
}

.gallery .slick-prev:before,
.gallery .slick-next:before {
    color: #353535;
    font-size: 20px;

}

@media (min-width: 768px) {
